//What is the largest prime factor of the number 600851475143 ?


 long n = 600851475143;
            int x = 3;
            while (n > 1)
            {
                if (n % x == 0)
                {
                    n = n / x;                    
                }
                else
                    x += 2;
            }
            Console.WriteLine(x);
            Console.ReadLine();
