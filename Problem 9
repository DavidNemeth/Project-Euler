public void Problem9 ()
        {
            //There exists exactly one Pythagorean triplet for which a + b + c = 1000.
            //Find the product abc.
            for (int a = 1; a < 500; a++)
                for (int b = 2; b < a; b++)
                {
                    if (a*a + b*b == (1000 - a - b)*(1000 - a - b))
                        Console.WriteLine(a*b*(1000 - a - b)); 
                }
        }
